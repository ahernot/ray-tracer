    "def custom_countdown(n):\n",
    "    while n > 0:\n",
    "        yield n\n",
    "        n -= 1\n",
    "\n",
    "# Create a generator object\n",
    "my_generator = custom_countdown(5)\n",
    "\n",
    "# Iterate over the generator using a for loop\n",
    "for value in my_generator:\n",
    "    print(value)"
   ]
